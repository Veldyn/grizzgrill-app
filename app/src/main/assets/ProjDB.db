CREATE TABLE IF NOT EXISTS CUSTOMER (
	CUSTOMER_ID	INTEGER NOT NULL,
	CUSTOMER_NAME	TEXT,
	TABLE_ID	INTEGER,
	FOREIGN KEY(TABLE_ID) REFERENCES CUSTOMER,
	PRIMARY KEY(CUSTOMER_ID AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS EMPLOYEES (
	"EMPLOYEE_ID"	INTEGER NOT NULL,
	"EMPLOYEE_NAME"	TEXT,
	PRIMARY KEY("EMPLOYEE_ID" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "RTABLES" (
	"RTABLE_ID"	INTEGER NOT NULL,
	"RTABLE_SEATS"	INTEGER,
	"RTABLE_STATUS"	INTEGER NOT NULL,
	"EMPLOYEE_ID"	INTEGER,
	"CUSTOMER_ID"	INTEGER,
	FOREIGN KEY("CUSTOMER_ID") REFERENCES "CUSTOMER"("CUSTOMER_ID"),
	FOREIGN KEY("EMPLOYEE_ID") REFERENCES "EMPLOYEES"("EMPLOYEE_ID"),
	PRIMARY KEY("RTABLE_ID" AUTOINCREMENT)
);
